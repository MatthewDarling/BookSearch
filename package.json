    "dev": "shadow-cljs -A:dev watch app & yarn styles-dev",
    "release": "shadow-cljs -A:dev release app && yarn styles-release",
    "styles-dev": "onchange -o '> public/main.css && lightningcss --bundle public/main.css -o public/main.css' -i src/app/css/*.css -- cat src/app/css/*.css",
    "styles-release": "cat src/**/*.css > public/main.css && lightningcss --minify --bundle public/main.css -o public/main.css",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss src/app/core.css -o public/main.css --verbose -w",
    "figwheel:watch": "clj -M:dev",
    "develop": "run-p -l *:watch"
  },
